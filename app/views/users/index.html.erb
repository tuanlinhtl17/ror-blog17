<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <%= render user %>
    <%= link_to "delete", user, method: :delete,
                          data: { confirm: "You sure?" } %> |
    <%= link_to "edit", edit_user_path(user) %>
    </li>
  <% end %>
</ul>
